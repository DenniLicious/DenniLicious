      <svarsalternativ svar="false">Applicering</svarsalternativ>
      <svarsalternativ svar="true">Appreciering</svarsalternativ>
    </fraga>
    <fraga id="7">
      <text>Vad är prisbasbeloppet (PBB) 2016?</text>
      <svarsalternativ svar="false">39 600kr</svarsalternativ>
      <svarsalternativ svar="true">44 300kr</svarsalternativ>
      <svarsalternativ svar="false">47 200kr</svarsalternativ>
    </fraga>
    <fraga id="8">
      <text>Vilken Bank är det här?</text>
      <bild img="/images/bjornbanck.jpg"></bild>
      <svarsalternativ svar="false">Swedbank</svarsalternativ>
      <svarsalternativ svar="false">Danske bank</svarsalternativ>
      <svarsalternativ svar="true">Björn Banck</svarsalternativ>
