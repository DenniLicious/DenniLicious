<?xml version="1.0" encoding="utf-8" ?>

<!--rotelement-->
<fragor>
    <!--föräldraelement-->
    <kategori typ="Produkter och hantering av kundens affärer" prov="aku2016">
      <!--barnelement-->
      <fraga id="1">
        <!--attribut-->
        <text>Vilket år grundades Riksbanken? 1668</text>
        <hjalp></hjalp>
        <bild></bild>
        <svarsalternativ1 svar="true">1668</svarsalternativ1>
        <svarsalternativ2 svar="false">1768</svarsalternativ2>
        <svarsalternativ3 svar="false">1868</svarsalternativ3>
      </fraga>
      <fraga id="2">
        <text>Är det här den nya 50-kronorssedeln?</text>
        <hjalp>Två alternativ ska markeras för att få poäng på frågan.</hjalp>
        <bild>/images/zlatan.jpg"</bild>
        <svarsalternativ1 svar="false">Ja</svarsalternativ1>
        <svarsalternativ2 svar="true">Nej</svarsalternativ2>
        <svarsalternativ3 svar="true">Nej, men det borde vara</svarsalternativ3>
      </fraga>
      <fraga id="3">
        <text>Vad väger en 1000-kronorssedel?</text>
        <hjalp></hjalp>
        <bild></bild>
        <svarsalternativ1 svar="true">1,01 gram</svarsalternativ1>
        <svarsalternativ2 svar="false">2,02 gram</svarsalternativ2>
        <svarsalternativ3 svar="false">3,03 gram</svarsalternativ3>
      </fraga>
      <fraga id="4">
        <text>Hur mycket motsvarade 5000kr för 100 år sedan?</text>
        <hjalp></hjalp>
        <bild></bild>
        <svarsalternativ1 svar="false">50 747kr</svarsalternativ1>
        <svarsalternativ2 svar="true">199 043kr</svarsalternativ2>
        <svarsalternativ3 svar="false">475 632kr</svarsalternativ3>
      </fraga>
      <fraga id="5">
        <text>Vad är de svenska sedlarna tillverkade av? </text>
        <hjalp></hjalp>
        <bild></bild>
        <svarsalternativ1 svar="true">Råbomull</svarsalternativ1>
        <svarsalternativ2 svar="false">Silke</svarsalternativ2>
        <svarsalternativ3 svar="false">Papper</svarsalternativ3>
      </fraga>
    </kategori>

    <kategori typ="Ekonomi - nationalekonomi, finansiell ekonomi och privatekonomi" prov="aku2016">
      <fraga id="6">
        <text>Vad kallas det när en valutas värde stiger i förhållande till andra valutors?</text>
        <hjalp></hjalp>
        <bild></bild>
        <svarsalternativ1 svar="false">Depreciering</svarsalternativ1>
        <svarsalternativ2 svar="false">Applicering</svarsalternativ2>
        <svarsalternativ3 svar="true">Appreciering</svarsalternativ3>
      </fraga>
      <fraga id="7">
        <text>Vad är prisbasbeloppet (PBB) 2016?</text>
        <hjalp></hjalp>
        <bild></bild>
        <svarsalternativ1 svar="false">39 600kr</svarsalternativ1>
        <svarsalternativ2 svar="true">44 300kr</svarsalternativ2>
        <svarsalternativ3 svar="false">47 200kr</svarsalternativ3>
      </fraga>
      <fraga id="8">
        <text>Vilken Bank är det här?</text>
        <hjalp></hjalp>
        <bild>/images/bjornbanck.jpg"</bild>
        <svarsalternativ1 svar="false">Swedbank</svarsalternativ1>
        <svarsalternativ2 svar="false">Danske bank</svarsalternativ2>
        <svarsalternativ3 svar="true">Björn Banck</svarsalternativ3>
      </fraga>
      <fraga id="9">
        <text>Vad betyder outsourcing?</text>
        <hjalp></hjalp>
        <bild></bild>
        <svarsalternativ1 svar="false">Att begära företrädesrätt vid försäljning</svarsalternativ1>
        <svarsalternativ2 svar="true">Att lägga ut delar av sin verksamhet på entreprenad</svarsalternativ2>
        <svarsalternativ3 svar="false">Att sälja ut kända märken</svarsalternativ3>
      </fraga>
      <fraga id="10">
        <text>Vad händer med penningvärdet vid inflation?</text>
        <hjalp></hjalp>
        <bild></bild>
        <svarsalternativ1 svar="false">Ökar</svarsalternativ1>
        <svarsalternativ2 svar="false">Ingenting</svarsalternativ2>
        <svarsalternativ3 svar="true">Minskar</svarsalternativ3>
      </fraga>
    </kategori>  

  <kategori typ="Etik och regelverk" prov="aku2016">
    <fraga id="11">
      <text>Hur mycket krävs i kontantinsats enligt Aktiebolagslagen för att starta aktiebolag?</text>
      <hjalp></hjalp>
      <bild></bild>
      <svarsalternativ1 svar="true">50 000kr</svarsalternativ1>
      <svarsalternativ2 svar="false">100 000kr</svarsalternativ2>
      <svarsalternativ3 svar="false">150 000kr</svarsalternativ3>
    </fraga>
    <fraga id="12">
      <text>Vem har rätt att delta på en årsstämma?</text>
      <hjalp></hjalp>
      <bild></bild>
      <svarsalternativ1 svar="false">Vem som helst</svarsalternativ1>
      <svarsalternativ2 svar="true">Den som äger minst en aktie i bolaget</svarsalternativ2>
      <svarsalternativ3 svar="false">Den som sitter i bolagets styrelse</svarsalternativ3>
    </fraga>
    <fraga id="13">
      <text>Vad betyder ocker?</text>
      <hjalp></hjalp>
      <bild></bild>
      <svarsalternativ1 svar="false">Att utnyttja insiderinformation</svarsalternativ1>
      <svarsalternativ2 svar="true">Att göra oskälig vinst</svarsalternativ2>
      <svarsalternativ3 svar="false">Att missbruka ett förtroende</svarsalternativ3>
    </fraga>
    <fraga id="14">
      <text>Vad kallas kortbedrägeri med ett annat ord?</text>
      <hjalp></hjalp>
      <bild></bild>
      <svarsalternativ1 svar="true">Skimning</svarsalternativ1>
      <svarsalternativ2 svar="false">Skuggning</svarsalternativ2>
      <svarsalternativ3 svar="false">Skyfall</svarsalternativ3>
    </fraga>
    <fraga id="15">
      <text>Vilken bank har den här logotypen?</text>
      <hjalp></hjalp>
      <bild>/images/logoutext.png</bild>
      <svarsalternativ1 svar="false">SEB</svarsalternativ1>
      <svarsalternativ2 svar="false">Skandiabanken</svarsalternativ2>
      <svarsalternativ3 svar="true">JE Banken</svarsalternativ3>
    </fraga>
  </kategori>
</fragor>