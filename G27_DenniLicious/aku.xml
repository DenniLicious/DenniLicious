<?xml version="1.0" encoding="utf-8" ?>

<!--rotelement-->
<fragor>
  <!--föräldraelement-->
  <kategori typ="Produkter och hantering av kundens affärer">
    <!--barnelement-->
    <fraga id="1">
      <!--attribut-->
      <text>Vilket år grundades Riksbanken? 1668</text>
      <svarsalternativ svar="true">1668</svarsalternativ>
      <svarsalternativ svar="false">1768</svarsalternativ>
      <svarsalternativ svar="false">1868</svarsalternativ>
    </fraga>
    <fraga id="2">
      <text>Är det här den nya 50-kronorssedeln?</text>
      <hjalp>Två alternativ ska markeras för att få poäng på frågan.</hjalp>
      <bild img="/images/zlatan.jpg"></bild>
      <svarsalternativ svar="false">Ja</svarsalternativ>
      <svarsalternativ svar="true">Nej</svarsalternativ>
      <svarsalternativ svar="true">Nej, men det borde vara</svarsalternativ>
    </fraga>
    <fraga id="3">
      <text>Vad väger en 1000-kronorssedel?</text>
      <svarsalternativ svar="true">1,01 gram</svarsalternativ>
      <svarsalternativ svar="false">2,02 gram</svarsalternativ>
      <svarsalternativ svar="false">3,03 gram</svarsalternativ>
    </fraga>
    <fraga id="4">
      <text>Hur mycket motsvarade 5000kr för 100 år sedan?</text>
      <svarsalternativ svar="false">50 747kr</svarsalternativ>
      <svarsalternativ svar="true">199 043kr</svarsalternativ>
      <svarsalternativ svar="false">475 632kr</svarsalternativ>
    </fraga>
    <fraga id="5">
      <text>Vad är de svenska sedlarna tillverkade av? </text>
      <svarsalternativ svar="true">Råbomull</svarsalternativ>
      <svarsalternativ svar="false">Silke</svarsalternativ>
      <svarsalternativ svar="false">Papper</svarsalternativ>
    </fraga>
  </kategori>

